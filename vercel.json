{
  "name": "ai-vision",
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node",
      "maxDuration": 300
    }
  },
  "rewrites": [
    { "source": "/analyze-image", "destination": "/api/analyze-image" },
    { "source": "/analyze-image-file", "destination": "/api/analyze-image-file" },
    { "source": "/transcribe-audio", "destination": "/api/transcribe-audio" },
    { "source": "/analyze-images-group", "destination": "/api/analyze-images-group" },
    { "source": "/server-status", "destination": "/api/server-status" }
  ]
} 